{
  "routes": [
    {
      "route": "/index.html",
      "rewrite": "/api/root"
    },
    {
      "route": "/learning-areas/*",
      "rewrite": "/api/engage"
    },
    {
      "route": "/login",
      "rewrite": "/api/login/aad"
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "responseOverrides": {
    "404": {
      "redirect": "/index.html"
    }
  }
}