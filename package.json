{
  "name": "learn-do-share",
  "version": "1.0.0",
  "description": "Track what you're learning, doing, and sharing",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "validate:display": "tsc --noEmit -p ./display",
    "validate:backstage": "tsc --noEmit -p ./api",
    "validate": "npm run validate:display && npm run validate:backstage",
    "build:display": "npm run validate:display && vite build",
    "start:server": "node --loader esbuild-node-loader --no-warnings ./local/backstage/index.ts",
    "start:display": "vite",
    "waitFor:server": "wait-on tcp:7664",
    "start": "nf start -p 7664",
    "test": "node --enable-source-maps --loader esbuild-node-loader --no-warnings ./behaviors/runner.ts",
    "test:debug": "DEBUG=true npm run test",
    "prepare": "husky install"
  },
  "author": "Brian Watkins",
  "license": "MIT",
  "devDependencies": {
    "@azure/functions": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@zeit/cosmosdb-server": "^0.13.2",
    "autoprefixer": "^10.4.4",
    "chai": "^4.3.6",
    "esbehavior": "^1.2.3",
    "esbuild-node-loader": "^0.8.0",
    "express": "^4.17.3",
    "foreman": "^3.0.1",
    "husky": "^7.0.0",
    "playwright": "^1.19.2",
    "postcss": "^8.4.12",
    "redux": "^4.1.2",
    "snabbdom": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "wait-on": "^6.0.1"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "dependencies": {
    "@azure/cosmos": "^3.15.1",
    "@microsoft/applicationinsights-web": "^2.7.4",
    "hast-util-select": "^5.0.1",
    "immer": "^9.0.12",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  }
}
